<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
  <class name="toby.service.sql.Sqlmap">
    <map-to xml="/sql/sql-map"/>
    <field name="sql" type="toby.service.sql.SqlType" required="true" collection="arraylist">
      <bind-xml name="sql" node="element"/>
    </field>
  </class>
  <class name="toby.service.sql.SqlType">
    <map-to xml="sql"/>
    <field name="key" type="string" required="true">
      <bind-xml name="key" node="attribute"/>
    </field>
    <field name="value" type="string" required="true">
      <bind-xml node="text"/>
    </field>
  </class>
</mapping>